{
  "name": "multipassmanager",
  "displayName": "Multipass Manager",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.1.36"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "multipass",
          "title": "Multipass",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "multipass": [
        {
          "id": "multipassList",
          "name": "Instances"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.startInstance",
        "title": "Start Instance",
        "category": "Multipass"
      },
      {
        "command": "multipass.stop",
        "title": "Stop Instance"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "extension.startInstance",
          "group": "multipass@1"
        },
        {
          "command": "multipass.stop",
          "group": "multipass@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "mocha": "^10.2.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3",
    "vscode": "^1.1.34"
  },
  "dependencies": {
    "@types/node": "^20.10.6"
  }
}
